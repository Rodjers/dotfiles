execute pathogen#infect()
syntax on
set shiftwidth=2
set tabstop=2
set expandtab
set relativenumber
inoremap <C-j> <C-n>
inoremap <C-k> <C-p>
inoremap jk <ESC>
nmap <C-j> o<ESC>k
nmap <C-k> O<ESC>j
map <C-n> :NERDTreeToggle<CR>
let g:neocomplete#enable_at_startup = 1
inoremap <expr><TAB>  pumvisible() ? "\<C-n>" : "\<TAB>"
filetype indent plugin on
set omnifunc=syntaxcomplete#Complete
